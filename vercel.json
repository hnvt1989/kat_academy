{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "installCommand": "npm install",
  "devCommand": "npm run dev:vite",
  "rewrites": [
    {
      "source": "/((?!mathProblems\\.json|assets/).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/mathProblems.json",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ]
    }
  ]
} 